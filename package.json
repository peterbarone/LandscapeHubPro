{
  "name": "landscapehub-pro",
  "version": "0.1.0",
  "description": "A modern AI-powered SaaS platform for landscaping businesses",
  "private": true,
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:init:test": "cross-env NODE_ENV=test npm run db:reset",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=tests/integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/LandscapingSaaS.git"
  },
  "keywords": [
    "landscaping",
    "saas",
    "ai",
    "property-management"
  ],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/your-username/LandscapingSaaS/issues"
  },
  "homepage": "https://github.com/your-username/LandscapingSaaS#readme",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1523.0",
    "node-fetch": "^2.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "cross-env": "^7.0.3",
    "sequelize-cli": "^6.6.2",
    "eslint-plugin-jest": "^27.4.0",
    "faker": "^5.5.3",
    "jest-extended": "^4.0.1"
  }
}
