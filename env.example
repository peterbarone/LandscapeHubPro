# Database Configuration
DB_HOST=postgres://username:password@hostname:5432/landscapehub
# If not using connection string, use these:
# DB_USER=your_db_username
# DB_PASSWORD=your_db_password
# DB_NAME=landscapehub
# DB_HOST=localhost
DB_PORT=5432

# MinIO Configuration
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=your_access_key
MINIO_SECRET_KEY=your_secret_key
MINIO_BUCKET=landscapehub
MINIO_USE_SSL=false

# App Configuration
NODE_ENV=development
PORT=3000
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION=24h
API_VERSION=v1
